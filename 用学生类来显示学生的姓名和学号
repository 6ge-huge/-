#include <iostream>
#include <string>
using namespace std;
class stu {
public:string name1; int numm;
      string n() {
          name1; return name1;
      }
      int m() {
          numm; return numm;
      }



};



  
int main() {
    while (true) {
        int i = 0; cin >> i;
        if (i != 0) {
            stu si;
            string name; cin >> name; si.name1 = name;
            int num; cin >> num; si.numm = num;
            cout << "学生姓名"<< si.n() << "学号"<<  si.m() << endl;
        }
        else { break; };

    }
